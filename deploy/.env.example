### Copy this to `.env` and fill in values before running deploy scripts
# RPC endpoint (Arbitrum mainnet or testnet)
RPC_URL="https://arb1.arbitrum.io/rpc"
# Optional override for Goerli/testnet RPC
GOERLI_RPC_URL="https://goerli-rollup.arbitrum.io/rpc"

# Private key for the deployer (0x-prefixed). Keep secret.
DEPLOYER_KEY="0xYOUR_PRIVATE_KEY_HERE"
